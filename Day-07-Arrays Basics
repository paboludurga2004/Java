class Array {
    public static void main(String[] args){
        int[] arr={1,5,10,15,20};
        int n=arr.length;
        
        for (int i=0;i<n;i++){
            System.out.print(arr[i]+" ");
        }
    }
}

output:-
1 5 10 15 20 

class ArrayReverse {
    public static void main(String[] args){
        int[] arr={1,5,10,15,20};
        int n=arr.length;
        for (int i=n-1;i>=0;i--){
            System.out.print(arr[i]+" ");
        }
    }
}

output:-
20 15 10 5 1 

