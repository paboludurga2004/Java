class Main {
    public static void main(String[] args){
        int count=0;
        int N=18;
        
        for (int i=1;i<=N;i++){
            if (N%i==0){
                count++;
            }
        }
        if (count==2){
            System.out.println("Yes");
        }
        else{
            System.out.println("No");
        }
    }
}
